{
    "name": "rule-lint",
    "version": "v1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/xulei131401/rule-lint"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.59.9",
        "eslint": "^8.0.1",
        "eslint-config-jammy": "^0.0.11",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "stylelint": "^15.7.0",
        "stylelint-config-recess-order": "^4.2.0",
        "stylelint-config-standard-scss": "^9.0.0",
        "typescript": "*",
        "vue": "^3.3.4",
        "vue-eslint-parser": "^9.3.1"
    },
    "main": "index.js",
    "files": [
        "index.js",
        "./src/eslint/basic.js",
        "./src/eslint/react-ts.js",
        "./src/eslint/vue-ts.js",
        "./src/eslint/typescript.js"
    ],
    "peerDependencies": {
        "eslint": ">=8.14.0",
        "typescript": "^4.6.3"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "prepare": "husky install",
        "lint": "eslint src --ext .js"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}